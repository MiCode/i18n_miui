<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Поиск</string>

    <!-- Search widget -->
    <string name="search_widget">Поиск</string>

    <!-- Text in the dialog for confirming whether to remove an item from the search
         history. [CHAR_LIMIT=NONE] -->
    <string name="remove_from_history">Удалить из истории поиска?</string>

    <!-- Toast text shown when the app fails to remove an item from
         the search history. [CHAR_LIMIT=60] -->
    <string name="remove_from_history_failed">Не удалось удалить объект из истории поиска.</string>

    <!-- Source selector -->
    <string name="corpus_selection_heading">Поиск</string>
    <string name="corpus_label_global">Все</string>

    <!-- Name of the combined Web source shown in QSB -->
    <string name="corpus_label_web">Интернет</string>
    <!-- Description of the combined Web source shown in QSB -->
    <string name="corpus_description_web">Интернет и закладки</string>

    <!-- Name of the Apps source shown in QSB -->
    <string name="corpus_label_apps">Приложения</string>
    <!-- Settings description for the Apps source shown in QSB -->
    <string name="corpus_description_apps">Названия установленных приложений</string>
    <!-- Search hint for the Apps source shown in QSB -->
    <string name="corpus_hint_apps">Поиск приложений</string>

    <!-- Search settings stuff -->

    <!-- Search settings menu item -->
    <string name="menu_settings">Настройки поиска</string>

    <!-- Search settings title -->
    <string name="search_settings">Настройки поиска</string>

    <!-- Title for Google Account category of search settings.
         [CHAR LIMIT=20] -->
    <string name="google_settings_category_title">Поиск Google</string>

    <!-- Title for 'device' category of search settings 
        [CHAR LIMIT=20] -->
    <string name="device_settings_category_title">Очистить ярлыки</string>

    <!-- Title for 'system search' category of search settings -->
    <string name="system_search_category_title">Телефон</string>

    <!-- Title and summary for 'search sources' link for system search -->
    <string name="search_sources">Источники поиска</string>
    <string name="search_sources_summary">Выберите источники, по которым будет осуществляться поиск</string>

    <!-- Title and summary used for the option to clear search shortcuts from the system
         search history -->
    <string name="clear_shortcuts">Очистить ярлыки</string>
    <string name="clear_shortcuts_summary">Очистка ярлыков недавно выбранных результатов поиска</string>

    <!--[CHAR LIMIT=200] Title used in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_prompt">"Очистить ярлыки недавно выбранных результатов поиска?\n\nЧтобы очистить историю браузера, воспользуйтесь настройками браузера."</string>
    <!--[CHAR LIMIT=30] Text used on the button to clear shortcuts in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_agree">Очистить ярлыки</string>
    <!--[CHAR LIMIT=30] Text used on the button to cancel in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_disagree">"Не очищать ярлыки"</string>

    <!-- Help menu item. Opens a web page with information about how to use Quick Search Box.
         [CHAR LIMIT=20] -->
    <string name="menu_help">Справка</string>

    <!-- Search source label for the Google search source. -->
    <string name="google_search_label">Google</string>

    <!-- The search activity label. -->
    <string name="google_app_label">Поиск Google</string>

