    <string name="clear_shortcuts">Bersihkan pintas</string>
    <string name="clear_shortcuts_summary">Bersihkan pintas menjadi saran pencarian yang dipilih</string>

    <!--[CHAR LIMIT=200] Title used in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_prompt">"Apakah anda ingin membersihkan semua pintas ke saran pencarian yang dipilih?"\n\n"Untuk menhapus riwayat Browser, gunakan pengaturan di Browser."</string>
