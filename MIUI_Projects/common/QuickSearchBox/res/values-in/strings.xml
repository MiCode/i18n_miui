    <string name="remove_from_history_failed">Tidak dapat menghapusnya dari riwayat.</string>

    <!-- Source selector -->
    <string name="corpus_selection_heading">Cari</string>
    <string name="corpus_label_global">Semua</string>

    <!-- Name of the combined Web source shown in QSB -->
    <string name="corpus_label_web">Web</string>
    <!-- Description of the combined Web source shown in QSB -->
    <string name="corpus_description_web">Pencarian dan penanda web</string>

    <!-- Name of the Apps source shown in QSB -->
    <string name="corpus_label_apps">Apl</string>
    <!-- Settings description for the Apps source shown in QSB -->
    <string name="corpus_description_apps">Nama aplikasi yang dipasang</string>
    <!-- Search hint for the Apps source shown in QSB -->
    <string name="corpus_hint_apps">Cari apl</string>

    <!-- Search settings stuff -->

    <!-- Search settings menu item -->
    <string name="menu_settings">Pengaturan pencarian</string>

    <!-- Search settings title -->
    <string name="search_settings">Pengaturan pencarian</string>

    <!-- Title for Google Account category of search settings.
         [CHAR LIMIT=20] -->
    <string name="google_settings_category_title">Google Search</string>

    <!-- Title for 'device' category of search settings 
